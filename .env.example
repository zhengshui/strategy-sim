# Model Configuration
# OpenAI API Key for AutoGen agents
OPENAI_API_KEY=sk-your-openai-api-key-here

# Model provider (openai, anthropic, etc.)
MODEL_PROVIDER=openai

# Model name to use
MODEL_NAME=gpt-4o

# Application Settings
# Debug mode (true/false)
DEBUG=true

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Chainlit Configuration
# Secret key for Chainlit authentication
CHAINLIT_AUTH_SECRET=your-secret-key-here

# Host for Chainlit app
CHAINLIT_HOST=0.0.0.0

# Port for Chainlit app
CHAINLIT_PORT=8000

# Optional: Database Configuration
# For session persistence and decision history
DATABASE_URL=sqlite:///./strategysim.db

# Optional: External APIs
# For enhanced market research and legal compliance
MARKET_DATA_API_KEY=your-market-data-api-key
LEGAL_DATABASE_API_KEY=your-legal-database-api-key

# Optional: Monitoring and Analytics
# For tracking agent performance and user interactions
ANALYTICS_ENABLED=false
ANALYTICS_API_KEY=your-analytics-api-key

# Optional: Email Configuration
# For sending decision reports
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password